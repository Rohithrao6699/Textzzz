generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String
  password String
  name     String
  RoomId   Int?
  Room     Room?  @relation(fields: [RoomId], references: [id])
}

model Room {
  id   Int    @id @default(autoincrement())
  name String
  User User[]
}
